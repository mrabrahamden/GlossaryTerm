        <Canvas x:Name="CrosswordCanvas" Margin="15,15,15,15" DockPanel.Dock="Top">
        </Canvas>
        <ScrollViewer x:Name="AnswersScrollViewer">
        </ScrollViewer>
    </DockPanel>
</Window>
