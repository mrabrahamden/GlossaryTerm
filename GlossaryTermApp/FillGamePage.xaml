        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GlossaryTermApp"
        mc:Ignorable="d"
        Title="FillGamePage" MinHeight="500" MinWidth="800">
    <Grid>
        <ScrollViewer x:Name="scrollViewer" Visibility="Visible"  HorizontalAlignment="Left" MinHeight="410" VerticalAlignment="Top" MinWidth="772" Margin="10,10,0,0">
            <StackPanel x:Name="stackPanelOutput" Orientation="Vertical"/>
        </ScrollViewer>
        <Button x:Name="BtnOk" Content="Проверить" Height="33" FontSize="20" Margin="0,444,29,0" VerticalAlignment="Top" Click="Button_Click" HorizontalAlignment="Right" Width="123"/>
    </Grid>
</Window>
